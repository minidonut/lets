    "clipanion": "^3.2.0-rc.4",
    "dotenv": "^10.0.0",
    "execa": "^5",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "prompts": "^2.4.2",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2"
  }
